<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_jar" name="Create Jar for Project MacJavaUtils">
    <!--ANT 1.7 is required                                        -->
    <target name="create_jar">
        <jar destfile="MacJavaUtils.jar">
            <manifest>
                <attribute name="Main-Class" value="com.isti.util.mac.MacUtilFns"/>
            </manifest>
            <fileset dir="bin"/>
        </jar>
    </target>
</project>
